.wrapper{
    width: 100%;
}
.title{
    width: 100%;
    height: 88rpx;
    display: flex;
    justify-content: center
}
.title image{
    display: block;
    width: 44rpx;
    height: 44rpx;
    margin-top: 24rpx;
}
.title .date{
    margin: 0 40rpx;
    align-self: center;
    font-size: 30rpx;
    color: #666;
    letter-spacing: 2rpx;
}
.content{
    width: 100%;
}
.header{
    width: 100%;
    height: 88rpx;
    display: flex;
    
}
.header view{
    height: 100%;
    width: 107rpx;
    text-align: center;
    line-height: 88rpx;
    color: #333;
    font-size: 30rpx;
    box-sizing: border-box;
    border-bottom: 4rpx solid #ccc;
}
.header view.weekMark{
    border-bottom: 4rpx solid #ff9933;
}
.dateBox{
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    margin-top: 15rpx;
}
.dateBox .item{
    width: 107rpx;
    text-align: center;
    font-size: 30rpx;
    color: #333;
    margin-bottom: 30rpx;
    display: flex;
    justify-content: center;
}
.dateBox .item view{
    height: 60rpx;
    width: 60rpx;
    line-height: 60rpx;
    border-radius: 50%;
}
.dateBox .item.nowDay view{
    background-color: #ff9933;
}